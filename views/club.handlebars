<link rel="stylesheet" href="/styles/club.css" media="screen" title="no title" charset="utf-8">

<div class="container" id="sessionwrapper">
    <div class="row">
        <div class="col-md">
            {{{session}}}
        </div>
    </div>
</div>

<div class="container" id="club-title-wrapper">
    <div class="row">
        <div class="col-md" id="club-header">
            <h2>{{clubname}}</h2>
            <span class="top">{{{id}}}</span>
            <span class="top"><br>{{{message}}}</span>
        </div>
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-md" id="hmenu">
            {{{addbutton}}}
        </div>
    </div>
</div>   

<div class="container" id="clubwrapper">
    <div class="row">
        <div class="col-3" id="leftnav">
            <br><br>
            {{!-- <h4>{{clubname}}</h4> --}}
            {{!-- <span class="top">{{{id}}}</span><br><br> --}}
            Owner: <div id="owner"></div><br>
            Members: <span id="members"></span><br>
            <div id="club-member-list"></div><br>
        </div>
        <div class="col-9">
            <br><br>
            Upcoming Events:<br><br>
            <div id="club-event-brief"></div>
            {{!-- <h5>Club Members:</h5> --}}
            <br><br>
        </div>
    </div>
</div>

{{!-- <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script> --}}
<script src="/js/club.js"></script>